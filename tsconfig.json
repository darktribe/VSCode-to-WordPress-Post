{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "out",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "types": ["node", "vscode"]
    },
    "exclude": ["node_modules", ".vscode-test"]
  }